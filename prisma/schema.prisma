generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Club {
  URSINHOS
  FAISCA
  FLAMA
  TOCHA
  JV
  GQ7
}

model Member {
  id           String   @id @default(uuid())
  full_name    String
  birth_date   DateTime
  mother_name  String?
  father_name  String?
  phone        String?
  street       String?
  city         String?
  neighborhood String?
  zip_code     String?
  is_active    Boolean  @default(true)
  club         Club
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
}
